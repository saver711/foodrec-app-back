{
  "kind": "collectionType",
  "collectionName": "meals",
  "info": {
    "singularName": "meal",
    "pluralName": "meals",
    "displayName": "Meal",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "regex": ".*[a-zA-Z].*",
      "required": true
    },
    "restaurant": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::restaurant.restaurant",
      "inversedBy": "meals"
    },
    "likedBy": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "fav_meals"
    },
    "recommendations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::recommendation.recommendation",
      "mappedBy": "meal"
    },
    "images": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    }
  }
}
